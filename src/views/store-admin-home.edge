<!DOCTYPE html>
<html>
<head>
    <meta charset='utf-8'>
    <meta http-equiv='X-UA-Compatible' content='IE=edge'>
    <title>Store Admin</title>
    <meta name='viewport' content='width=device-width, initial-scale=1'>
    <link href="https://unpkg.com/tabulator-tables@6.3.0/dist/css/tabulator.min.css" rel="stylesheet">
 <script type="text/javascript" src="https://unpkg.com/tabulator-tables@6.3.0/dist/js/tabulator.min.js"></script>
    <link rel='stylesheet' type='text/css' media='screen' href='main.css'>
    <script src='main.js'></script>
</head>
<body>
    <a href="/store/create-store">Create</a>

    <div id="store-table"></div>
    
</body>
<script>
     

    let table = new Tabulator("#store-table", {
 	height:205,
 	data:{{{JSON.stringify(data)}}},
 	layout:"fitColumns", 
 	columns:[ 
	 	{title:"id", field:"id", width:150},
	 	{title:"itemQuantity", field:"itemQuantity"},
	 	{title:"itemName", field:"itemName"},
        {title:"itemSupplier", field:"itemSupplier"},
        {title:"inWarehouse", field:"inWarehouse", formatter:"tickCross"},
        {title:"dateAdded", field:"dateAdded"},
        {title:"dateUpdated", field:"dateUpdated"},
        {title:"update", field:"update", formatter:"html"},
        {title:"delete", field:"delete", formatter:"html"}
     	],
});

    

</script>
</html>